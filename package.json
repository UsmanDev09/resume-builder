{
  "name": "resume-editor-microfrontend",
  "private": true,
  "scripts": {
    "db:generate": "prisma generate --schema=./packages/database/prisma/schema.prisma",
    "build": "turbo run db:generate build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "@prisma/client": "^6.1.0",
    "prisma": "^6.1.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}