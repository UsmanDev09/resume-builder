{
    "name": "@resume/db",
    "version": "0.0.0",
    "scripts": {
        "db:generate": "prisma generate --schema=./packages/database/prisma/schema.prisma",
        "db:push": "prisma db push --skip-generate",
        "db:migrate": "prisma migrate dev --skip-generate",
        "db:deploy": "prisma migrate deploy"
    },
    "exports": {
        ".": "./src/index.ts"
    }
}