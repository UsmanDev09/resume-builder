{
    "name": "@resume/db",
    "version": "0.0.0",
    "scripts": {
        "db:generate": "npx prisma generate --schema=./prisma/schema.prisma",
        "db:push": "prisma db push --skip-generate",
        "db:migrate": "prisma migrate dev --skip-generate",
        "db:deploy": "prisma migrate deploy"
    },
    "dependencies": {
        "@prisma/client": "^6.1.0",
        "prisma": "^6.1.0"
    },
    "exports": {
        ".": "./src/index.ts"
    }
}